{
    "name": "eliseekn/laravel-metrics-demo",
    "type": "project",
    "description": "Demo project of Laravel Metrics.",
    "keywords": [
        "laravel-package",
        "charts",
        "laravel",
        "dashboard"
    ],
    "homepage": "https://github.com/eliseekn/laravel-metrics-demo",
    "license": "MIT",
    "authors": [
        {
            "name": "N'Guessan Kouadio Elis√©e",
            "email": "eliseekn@gmail.com",
            "homepage": "https://eliseekn.netlify.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "eliseekn/laravel-metrics": "v2.9.0-beta-2",
        "fruitcake/laravel-cors": "dev-develop",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
